{
  "compilerOptions": {
    "outDir": "./lib/js",
    "baseUrl": "./",
    "target": "ESNext",
    "module": "ES6",
    "noEmitOnError": false,
    "strict": true,
    "moduleResolution": "node",
    "noUncheckedIndexedAccess": true,
    "declarationMap": true,
    "sourceMap": true,
    "declaration": true,
    "declarationDir": "lib/types",
    "allowImportingTsExtensions": false,
  },
    "include": ["lib.ts","test/*"],
    "exclude": ["node_modules"]
}
